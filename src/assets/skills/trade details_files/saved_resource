				function showChatNow()
				{	 
				window.open('https://chat.SBIsec.com/cosmocall/SBI-chat.html', '','toolbar=no,statusbar=no,menubar=no,width='+(screen.width*0.995)+',height='+screen.height*0.90+',resizable=no,top=0,left=0');
				}
				
