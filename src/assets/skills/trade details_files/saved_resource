 //19 Aug: js logic for 2fa
 
  var twoFaModeJs = 'Y';
  console.log("twoFaModeJs: "+twoFaModeJs);
  var currPage = "1"; //1-> login page, 2-> otp page
  if(twoFaModeJs == "N"){
	  //use dob for 2fa 
	  $("#resendOTP").hide();
	  $("#backToLogin").hide();
	  $("#otpSubmit1").hide();
	 
  }
  else{
	$("#submitButton").hide(); //hiding login now button
	$("#resendOTP").hide();
	$("#backToLogin").hide();
	$("#dobfield").hide(); // hide dob field dobicon
  var servletOTP = "";
  var otpStartTime = "";
  }
  // otp submit click
	$("#otpSubmit1").click(function()
	{
		//let formCheck1 =  chkuserId2();
		
		let formCheck1 = false;
		if(OTPAsk == "First") {
			formCheck1 =  chkuserId2();
		} else if(OTPAsk == "Second") {
			formCheck1 =  true;
		}
		
		console.log("otp submit 1 clicked");
		if(formCheck1){
		$("#otpSubmit1").prop('disabled', true);
		$("#TandC").hide();
		//disable login form
		//disableLoginForm()
		var strUrl='/OTPValServletAction.do?method=sendOTP';  //DpSyncCall';
		$("#otpSubmit1").attr("disabled", true);
		var param = "&Channel="+"I"+
							"&userId="+$("#userId").val()+
							"&dob="+$("#dob").val()+
							"&password="+encodeURIComponent($("#password").val())+
							"&terminal=ITS"+
							"&BaseNo="+$("#BaseNo").val();
							
		$.ajax({
					url: strUrl,		
					type: 'POST',
					data: param,
					async: false,
					cache: false,
					dataType: "json",
					success: function(json) {
						//console.log(json);
					//$('#DpSyncBtn').attr("disabled", false);
					if(json["status"] === false)
					{
						alert(json["errmsg"]);
						//clearing login form 
						enableLoginForm();
						clearLoginForm();
						$("#otpSubmit1").prop('disabled', false);
						//ITSUtility.handleSessionExpiry(json);
						return;
					}
					//else if(json["status"]==its.constants.NODATA){
						
					//}
					else{
						if(json["status"]){
							
							currPage = "2"; //now on otp page
						document.getElementById("otpdiv").classList.add("otpDivBox");
						// json["status"] === true 
						// otp sent successfully 
					    //otp_pin
						//unhide otp input box
						$("#otpHideFields").hide();
						$("#otpmesg").show(); 
						$("#otpmesg1").show();
						$("#otpmesg2").show(); 
						$("#otpSubmit1").hide();
						document.getElementById("otp_pin").type = "password";
					    $("#otp_pin").show(); 
						//unhide otp validate button
						document.getElementById("validateOtp").type = "button";
						let startTime = new Date();
						otpStartTime = startTime.getTime()/1000;
					    $("#validateOtp").show();
						servletOTP=(json.otp);
						//console.log("servletOtp: "+servletOTP);
						//console.log("startTime: "+otpStartTime);
						alert("OTP has been sent successfully on your registered Mobile Number and Email ID. It will be valid for 5 minutes.\nKindly Note: OTP will be sent only on Indian Mobile Number.");
						//alert("OTP: "+servletOTP);
						$("#otp_pin").focus();
						$("#resendOTP").show();
						$("#backToLogin").show();
						return;
						}
						
						
						}},
					error: function(XMLHttpRequest, textStatus, errorThrown) {
					
						////alertify.alert("error" + errorThrown);
						////alertify.alert("textStatus" + textStatus);
					}
				});				

		}
		
	});
  
  
  
   function chkuserId2() 
			 {
				 console.log("inside chkuserId2");
				 document.getElementById("dob").value="11111111";

			  var frmname = "login_frm";
			  var theValue = document.getElementById("userId").value;//theForm.userId.value;
			  if ((theValue.length == 0) || (theValue == "")) {
				document.getElementById("userId").value="";//theForm.password.value = "";
				document.getElementById("dob").value="";//theForm.dob.value = "";
				alert("Please enter a valid username.");
				document.getElementById("userId").focus();//theForm.userId.focus();
				document.getElementById("userId").select();//theForm.userId.select();
				return false;
			  }
			  else if(document.getElementById("password").value == "") {
				alert("Please enter your password.");
				document.getElementById("password").focus();
				//theForm.password.focus();
				return false;
			  }
				else if(document.getElementById("password").value==""){ 
					alert("Please enter date of birth.");
					document.getElementById("dob").focus();//theForm.dob.focus();
					return false; 
				}
				else if(document.getElementById("dob").value !="" && !validateDate(document.getElementById("dob"))){
					document.getElementById("dob").focus();
					return false;
					}
				else if(!validateDate(document.getElementById("dob"))){
					 document.getElementById("dob").focus();
					return false;
					}
					else
					{
					encpt2();
					//disableSubmitButton(document.getElementById("otpSubmit1"));
				return true;
			  }
			 }
		function encpt2(){
			document.getElementsByClassName('fa-eye-slash').disabled=false;
			document.getElementById('password').setAttribute('type','password')
			document.getElementById('dob').setAttribute('type','password')
			
			var strURL = '/checkUtility.jsp';
			$.ajax({
				url: strURL,		
				type: 'POST',
				async: false,
				cache: false,
				dataType: "json",
				success: function(json) {
					var loginId = document.getElementById("userId").value;
					var dob = document.getElementById("dob").value;
					var randomHash=Math.floor(Math.random() * 115463345);
					var randomHash1 = randomHash.toString();
					var pwd=document.getElementById("password").value;
					var hash = sha256(randomHash1);
					var Hash1 = sha256(json["data"]["h2"]);
					var Hash2 = sha256(pwd);
					var Hash3 = sha256(loginId);
					var salt = hash;
			
					var aesEncryption = new AesEncryption(128, 1000);
					var encryptedString = aesEncryption.encrypt(salt, json["data"]["liv"], json["data"]["tech"], pwd);
					var encHash = aesEncryption.encrypt(Hash1, json["data"]["liv"], json["data"]["tech"], randomHash1);
					var encLogin = aesEncryption.encrypt(Hash2, json["data"]["liv"], json["data"]["tech"], loginId);
					var encDob = aesEncryption.encrypt(Hash3, json["data"]["liv"], json["data"]["tech"], dob);
					
					document.getElementById("password_enc").value = encryptedString;
					document.getElementById("BaseNo_enc").value = encHash;
					document.getElementById("userId_enc").value = encLogin;
					document.getElementById("dob_enc").value = encDob;

					
					document.getElementById("password").value = encryptedString;
					document.getElementById("BaseNo").value = encHash;
					document.getElementById("userId").value = encLogin;
					document.getElementById("dob").value = encDob;

				},
				error: function(XMLHttpRequest, textStatus, errorThrown) {
					alert(textStatus);
					alert(errorThrown);
				}
			});
		}
		   function clearLoginForm(){
			document.getElementById("password").value = "";
			document.getElementById("BaseNo").value = "";
			document.getElementById("userId").value = "";
			document.getElementById("userId").type = "text";
			//document.getElementById("dob").value = "";
		   } 
		   function disableLoginForm(){
			   $("#userId").prop('disabled', true);
			   $("#password").prop('disabled', true);
			   $("#dob").prop('disabled', true);
	
		   } 
		  function enableLoginForm(){
			   $("#userId").prop('disabled', false);
			   $("#password").prop('disabled', false);
			   $("#dob").prop('disabled', false);
	
		   } 
