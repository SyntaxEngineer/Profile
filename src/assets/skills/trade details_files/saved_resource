		   function validateOTP(trueOtp){ //Added for otp vapt
			  enableLoginForm();//arjun: imp for authenticate to work
			  let userEnteredOtp = document.getElementById("otp_pin").value;
			  //console.log("userEnteredOtp: "+userEnteredOtp);
			  let entryTime = new Date();
			  entryTime = entryTime.getTime()/1000;
			  let timeDiffSecs = entryTime - otpStartTime;
			  console.log("timeDiffSecs: "+timeDiffSecs);

			 
				  if(timeDiffSecs > 5*60){// in secs
					alert("OTP expired. Kindly resend to Login.!!!");
					window.location.reload();

				   }
				   else{
					   validateOTPServlet("T",trueOtp,userEnteredOtp);
				   }
		   }
		   
