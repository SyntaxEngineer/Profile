			function validateOTPServlet(validate,trueOtp,userEnteredOtp) 
	{
		console.log("validateOTPServlet() called !!!");
		var strUrl='/OTPValServletAction.do?method=validateOTP';
		
		var param = "&Channel="+"I"+
							"&userId="+$("#userId_enc").val()+
							"&dob="+$("#dob_enc").val()+
							"&password="+encodeURIComponent($("#password_enc").val())+
							"&terminal=ITS"+
							"&BaseNo="+$("#BaseNo_enc").val()+
							"&validation="+validate+"&trueOtp="+trueOtp+"&userEnteredOtp="+userEnteredOtp;
							
		$.ajax({
					url: strUrl,		
					type: 'POST',
					data: param,
					async: true,
					cache: false,
					dataType: "json",
					success: function(json) {
						console.log(json);
						if(json["status"] === true){
						if(json["islocked"] === true){
							alert("Your account is locked !");
							window.location.reload();
						}
					}
					if(json["validation"] === "F") 
					{
						alert("You have entered incorrect OTP. Try again!!!");
						disableLoginForm();
						document.getElementById("otp_pin").focus();
					}
					else if(json["validation"] === "T"){ 
						$("#validateOtp").prop('disabled', true);
						$("#submitButton").click(); //Normal Login button
					}

					else{
						//OTP EXPIRED 
						alert("OTP session expired, Try again");
						window.location.reload();
					}
				},
				error: function(XMLHttpRequest, textStatus, errorThrown) {
					console.log("textStatus::"+textStatus);
					console.log("errorThrown::"+errorThrown);
				}
			});				
		}
		
	function backToLogin(){
		window.location.reload();
	}
	$("#otp_pin").keypress(function(event) {
		//Arjun: when user enters otp and clicks enter key then validate otp button should get clicked.
        if (event.keyCode === 13) {
            $("#validateOtp").click();
        }
        });
  
	
	
	
	$('#login_frm').on('keypress ', function(e) {
  var keyCode = e.keyCode || e.which;
  if (keyCode === 13) {
     //alert(currPage);
	 if(currPage == "1"){
	$("#otpSubmit1").click();
	
    return false;
    //e.preventDefault();
	 }

  }
});
  
