				}

				function getDefaultPage(objSelect)
				{
				  var selectedValue = objSelect.options[objSelect.selectedIndex].value;
				  document.login_frm.jspName.value =selectedValue;              
				}
				
				function OpenLink(linkName)
				{
				 
				  var strURL="";
				  var height=350;
				  var width=600;
				  var screenPosX=20;
				  var screenPosY=20;
				  if(linkName =="B") strURL = "http://www.bseindia.com";
				  else if(linkName =="N") strURL = "http://www.nseindia.com";
				  else if(linkName =="S") strURL = "http://www.sebi.com";
				  else if(linkName =="D") strURL = "/"+"help/Disclaimer.html";
				  else if(linkName =="H") 
					  {
						 strURL = "/"+"help/SSL35.html";
						 width=screen.width;
						 height=(screen.height/2)-100;
						 screenPosX=0;
						 screenPosY=0;
					  }
				  else if(linkName =="Privacy") strURL = "/"+"help/Privacy.html";
				  else if(linkName =="Terms") strURL = "/"+"help/TermsCondition.html";
				  else if(linkName =="login") strURL = "/"+"help/LoginHelp.jsp";      
				  else if(linkName =="signup") strURL = "http://www.SBIsec.com/TradeAccount.aspx";
				  else if(linkName =="pan") strURL = "/"+"help/pan.html";
				  else if(linkName =="brok") strURL = "/"+"help/brok.html";
				  else if(linkName =="browser")
				  {
					strURL = "/"+"help/browser.html";
					width=(screen.height/2)+120;
					height=screen.height-100;
					screenPosX=10;
					screenPosY=10;
				  }
				  else if(linkName =="Demo")
				  {
					strURL = "http://www.sbisec.com/TradingDemo/index.html";  
					width=screen.width-100;
					height=screen.height-100;
					screenPosX=10;
					screenPosY=10;
				  } 
				  else if(linkName =="Password") 
				 {     
					strURL ="help/forgotFrame.jsp";
					width=screen.width-335;
					height=screen.height-60;
					screenPosX=10;
					screenPosY=10;
					Win_Name="_blank"
				  }
				  reference = window.open(strURL,"_blank","toolbar=no,location=no,"+"directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,screenX="+screenPosX+",screenY="+screenPosY+"copyhistory=yes,height="+height+", width="+width);				}
			
				function Opensbisec()
					{
						window.open("http://www.sbisec.com","_blank","toolbar=no,location=no,"+"directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,screenX=0,screenY=0"+"copyhistory=yes,height="+screen.height+", width="+screen.width);      
					} 

				function openNewWindow(strURL,width,height)
					{		   
						window.open(strURL ,"_blank","toolbar=no,location=no,"+"directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,"+"copyhistory=yes,height="+height+", width="+width);
					}

