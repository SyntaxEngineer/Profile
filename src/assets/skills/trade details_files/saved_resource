		function alpha(e) {
			var k;
			document.all ? k = e.keyCode : k = e.which;
			return ((k > 64 && k < 91) || (k > 96 && k < 123) || k == 8 || (k >= 48 && k <= 57));
		}
		
		
		//function added to allow numbers only in DOB field.
		function numbersonly_order(formControl,e)
		{
		//debugger;
			/* alert('navigator.appName---'+navigator.appVersion); */
			if(!e)
				var e = window.event;
			var keyCode;
			if(e.keyCode)
				keyCode=e.keyCode;
			else if(e.which)
				keyCode = e.which;
			var ctrlKeyPressed = e.ctrlKey;
			var shiftKeyPressed = e.shiftKey;
		   var pressedKey = String.fromCharCode(keyCode).toLowerCase();     
		   var fieldVal=formControl.value;
			 if (ctrlKeyPressed && (pressedKey == "c" || pressedKey == "v")) 
		   {        
			  return false;
		   }
			if ((keyCode<48||keyCode>57))
			{
				if(keyCode==37 || keyCode==39)
					return false;
				if(!(keyCode==8 || keyCode==9 || keyCode==13 || (keyCode==9 && shiftKeyPressed))) 
				{
					
					//alert("keyCode = "+keyCode);
					return false;
				}
					
			}
			 if ((navigator.appName == "Netscape") && !document.ALL) {
				  document.captureEvents(Event.KEYPRESS);
				  document.onkeypress = function (evt) {
				   if (evt.which==32)
				   {
						return false;
				   }
					   
				  }
				 }else{
			
				  if (event.keyCode==32)
				   {
						return false;
				   } 
				  
				 }
			return true;
		}

			 
				
			 function chkuserId(theForm) 
			 {
			  var frmname = theForm.name;
			  var theValue = theForm.userId.value;
			  theForm.dob.value="11111111";
			  
			  if ((theValue.length == 0) || (theValue == "")) {
				theForm.password.value = "";
				theForm.dob.value = "";
				alert("Please enter a valid username.");
				theForm.userId.focus();
				theForm.userId.select();
				return false;
			  }
			  else if(theForm.password.value == "") {
				alert("Please enter your password.");
				theForm.password.focus();
				return false;
			  }
				else if(theForm.dob.value==""){ 
					alert("Please enter date of birth.");
					theForm.dob.focus();
					return false; 
				}
				else if(theForm.dob.value !="" && !validateDate(theForm.dob)){
					theForm.dob.focus();
					return false;
					}
				else if(!validateDate(theForm.dob)){
					 theForm.dob.focus();
					return false;
					}
					else
					{
					encpt(theForm);
					disableSubmitButton(theForm.submitButton);
				return true;
			  }
			 }

			  function validateDate(input){
				var returnval=false;
				var dateval = input.value;
				
				var dayfield=dateval.substring(0,2);
				var monthfield=dateval.substring(2,4);
				var yearfield=dateval.substring(4,8);
				var dayobj = new Date(yearfield, monthfield-1, dayfield)
				
				if ((dayobj.getMonth()+1!=monthfield)||(dayobj.getDate()!=dayfield)||(dayobj.getFullYear()!=yearfield))
					alert("Invalid Day, Month, or Year range detected.Please correct and submit again.");
				else
					returnval=true
						
						
				if (returnval==false) input.select() 
					return returnval
				}
				function disableSubmitButton(buttonObj)
				{
				  buttonObj.disabled = true;
				}
				  
				  
			function encpt(loginForm){
				
				document.getElementsByClassName('fa-eye-slash').disabled=false;
				
				document.getElementById('password').setAttribute('type','password');
				document.getElementById('dob').setAttribute('type','password');
					
				document.getElementById("password").value = document.getElementById("password_enc").value;
				document.getElementById("BaseNo").value = document.getElementById("BaseNo_enc").value;
				document.getElementById("userId").value = document.getElementById("userId_enc").value;
				document.getElementById("dob").value = document.getElementById("dob_enc").value;
				
				loginForm.userId.type = "password";
				loginForm.dob.value = document.getElementById("dob").value;
				loginForm.userId.value = document.getElementById("userId").value;
				
			
				/* var strURL = '/checkUtility.jsp';
				$.ajax({
					url: strURL,		
					type: 'POST',
					async: false,
					cache: false,
					dataType: "json",
					success: function(json) {
						var loginId = loginForm.userId.value;
						var dob = loginForm.dob.value;
						var randomHash=Math.floor(Math.random() * 115463345);//document.getElementById("LoginID").value;
						var randomHash1 = randomHash.toString();
						var pwd=document.getElementById("password").value;
						var hash = sha256(randomHash1);
						var Hash1 = sha256(json["data"]["h2"]);
						var Hash2 = sha256(pwd);
						var Hash3 = sha256(loginId);
						var salt = hash;
			
						var aesEncryption = new AesEncryption(128, 1000);
						var encryptedString = aesEncryption.encrypt(salt, json["data"]["liv"], json["data"]["tech"], pwd);
						var encHash = aesEncryption.encrypt(Hash1, json["data"]["liv"], json["data"]["tech"], randomHash1);
						var encLogin = aesEncryption.encrypt(Hash2, json["data"]["liv"], json["data"]["tech"], loginId);
						var encDob = aesEncryption.encrypt(Hash3, json["data"]["liv"], json["data"]["tech"], dob);

						document.getElementById("password").value = encryptedString;
						document.getElementById("BaseNo").value = encHash;
						loginForm.userId.value = encLogin;
						loginForm.userId.type = "password";
						loginForm.dob.value = encDob;

						document.getElementById("dob").value = encDob;
						document.getElementById('password').setAttribute('type','password')
						document.getElementById('dob').setAttribute('type','password')
					},
					error: function(XMLHttpRequest, textStatus, errorThrown) {
						alert(textStatus);
						alert(errorThrown);
					}
				}); */
			}
		   
			function removeSpace()
			 {
				 if ((navigator.appName == "Netscape") && !document.ALL) {
				  document.captureEvents(Event.KEYPRESS);
				  document.onkeypress = function (evt) {
				   if (evt.which==32)
				   {
						return false;
				   }       
				  }
				 }else{
			
				  if (event.keyCode==32)
				   {
						return false;
				   } 
				 }
			 }  


